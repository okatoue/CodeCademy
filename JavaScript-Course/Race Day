let raceNumber = Math.floor(Math.random() * 1000);

let early = true;

let runnersAge = 19;

if (runnersAge > 18) {
  early ? console.log(`Race starts at 9:30 am! Your race number is ${raceNumber += 1000}`) : console.log(`Late adults run at 11:00 am! Your race number is ${raceNumber}`)
} else if (runnersAge < 18) {
  console.log(`Your race starts at 12:30 pm! Your race   number is ${raceNumber}`)
} else {
  console.log('Please see the front desk') 
}
